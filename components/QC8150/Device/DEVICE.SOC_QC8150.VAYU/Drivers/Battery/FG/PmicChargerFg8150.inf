;
; PmicChargerFg8150.inf
;

[Version]
Signature="$WINDOWS NT$"
Class=System ; TODO: specify appropriate Class
ClassGuid={4d36e97d-e325-11ce-bfc1-08002be10318} ; TODO: specify appropriate ClassGuid
Provider=%ManufacturerName%
CatalogFile=PmicChargerFg8150.cat
DriverVer = 11/17/2022,21.22.58.223
PnpLockdown=1

[DestinationDirs]
DefaultDestDir = 12
PmicChargerFg8150_Device_CoInstaller_CopyFiles = 11

[SourceDisksNames]
1 = %DiskName%,,,""

[SourceDisksFiles]
PmicChargerFg8150.sys  = 1,,
;


;*****************************************
; Install Section
;*****************************************

[Manufacturer]
%ManufacturerName%=Standard,NTARM64

[Standard.NTARM64]
%PmicChargerFg8150.DeviceDesc%=PmicChargerFg8150_Device, ACPI\QCOM0580

[PmicChargerFg8150_Device.NT]
CopyFiles=Drivers_Dir

[Drivers_Dir]
PmicChargerFg8150.sys

;-------------- Service installation
[PmicChargerFg8150_Device.NT.Services]
AddService = PmicChargerFg8150,%SPSVCINST_ASSOCSERVICE%, PmicChargerFg8150_Service_Inst

; -------------- PmicChargerFg8150 driver install sections
[PmicChargerFg8150_Service_Inst]
DisplayName    = %PmicChargerFg8150.SVCDESC%
ServiceType    = 1               ; SERVICE_KERNEL_DRIVER
StartType      = 3               ; SERVICE_DEMAND_START
ErrorControl   = 1               ; SERVICE_ERROR_NORMAL
ServiceBinary  = %12%\PmicChargerFg8150.sys

;
;--- PmicChargerFg8150_Device Coinstaller installation ------
;

[PmicChargerFg8150_Device.NT.CoInstallers]
AddReg=PmicChargerFg8150_Device_CoInstaller_AddReg
CopyFiles=PmicChargerFg8150_Device_CoInstaller_CopyFiles

[PmicChargerFg8150_Device_CoInstaller_AddReg]
;


[PmicChargerFg8150_Device_CoInstaller_CopyFiles]
;


[PmicChargerFg8150_Device.NT.Wdf]
KmdfService =  PmicChargerFg8150, PmicChargerFg8150_wdfsect
[PmicChargerFg8150_wdfsect]
KmdfLibraryVersion = 1.15

[Strings]
SPSVCINST_ASSOCSERVICE= 0x00000002
ManufacturerName="MollyTek"
DiskName = "PmicChargerFg8150 Installation Disk"
PmicChargerFg8150.DeviceDesc = "PmicChargerFg8150 Device"
PmicChargerFg8150.SVCDESC = "PmicChargerFg8150 Service"
